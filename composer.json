{
  "name": "fredemmott/hhvm-autoload",
  "type": "composer-plugin",
  "require": {
    "composer-plugin-api": "^1.0",
    "hhvm": "~3.9",
    "91carriage/phpunit-hhi": "^5.1",
    "fredemmott/definition-finder": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": [
      "src/AutoloadMap.php",
      "src/Config.php"
    ]
  },
  "extra": {
    "class": [
      "FredEmmott\\AutoloadMap\\ComposerPlugin"
    ]
  }
}
